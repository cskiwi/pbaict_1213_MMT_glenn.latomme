<Window x:Class="GameAI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="800" Width="1000" MouseLeftButtonDown="DrawCanvas_MouseLeftButtonDown"
        MouseRightButtonDown="DrawCanvas_MouseRightButtonDown">

    <Grid>
        <Canvas x:Name="DrawCanvas" HorizontalAlignment="Center" Height="707" VerticalAlignment="Center" Width="972" Margin="10,53,10,10" Grid.ColumnSpan="2" />

        <Button Content="Generate" HorizontalAlignment="Left" Margin="10,22,0,0" VerticalAlignment="Top" Width="75"
                Click="Button_Click" />
        <Label Content="Time elapsed: " HorizontalAlignment="Left" Margin="90,22,0,0" VerticalAlignment="Top" />
        <Label x:Name="Time" Content="" HorizontalAlignment="Left" Margin="176,22,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.467,0.5" Width="150" />
        <Label Content="Path length: " HorizontalAlignment="Left" Margin="331,22,0,0" VerticalAlignment="Top" />
        <Label x:Name="Path" Content="" HorizontalAlignment="Left" Margin="408,22,0,0" VerticalAlignment="Top" Width="200"/>
        <Label Content="Tree length: " HorizontalAlignment="Left" Margin="613,22,0,0" VerticalAlignment="Top" />
        <Label x:Name="Tree" Content="" HorizontalAlignment="Left" Margin="690,22,0,0" VerticalAlignment="Top" Width="200"/>
    </Grid>
</Window>